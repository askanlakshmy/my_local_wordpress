                function onLoadJqueryLibrary(trigger, delayInterval, delayTimeout) {
                        var interval = setInterval(function () {
                                if (window.jQuery && window.jQuery.fn) {
                                        clearInterval(interval);
                                        trigger();
                                }
                        }, delayInterval);
                        setTimeout(function () {
                                clearInterval(interval);
                        }, delayTimeout);
                }

                function initVariation() {
                        onLoadJqueryLibrary(
                                function () {
                                        window[tag].waitForElement('#menu-main-menu-new .about-nav', function () {
                                                jQuery('.et-boc header').prepend(languageHTML);
                                                jQuery('#menu-main-menu-new .mega-menu').append(MegaNavProductsHTML);
                                                jQuery('#menu-main-menu-new .insights-nav').append(MegaNavInsightsEducationHTML);
                                                jQuery('#menu-main-menu-new .media-nav').append(MegaNavMediaHTML);
                                                jQuery('#menu-main-menu-new .about-nav').append(MegaNavAboutHarvestHTML);
                                        })

                                        window[tag].waitForElement('#mobile_menu2', function () {
                                                jQuery('#mobile_menu2').prepend(MobileNavAccordian);
                                        })

                                        window[tag].waitForElement('li.menu-item-has-children', function () {
                                                jQuery('li.menu-item-has-children > a:contains(Learn)').closest('li').addClass('learn-nav')[1];
                                                jQuery('li.menu-item > a:contains(Performance)').closest('li').addClass('performance')[1];
                                                jQuery('li.menu-item-has-children > a:contains(Media)').closest('li').addClass('media-nav')[1];
                                                jQuery('li.menu-item-has-children > a:contains(Insights)').closest('li').addClass('insights-nav')[1];
                                                jQuery('li.menu-item-has-children > a:contains(Harvest)').closest('li').addClass('about-nav')[1];
                                                jQuery('li.menu-item  > a:contains(French)').closest('li').addClass('francais-nav')[1];
                                                jQuery('.insights-nav > a').text('Insights & Education');
                                                jQuery('.about-nav > a').text('About Harvest');
                                                jQuery('.francais-nav > a').text('Francais');
                                                jQuery('.et_pb_menu_0_tb_header .et_pb_menu__search-button').text('Search');
                                        })


                                        window[tag].waitForElement('.mega-columns', function () {
                                                jQuery(".mega-columns:not(.column-one)").hover(
                                                        function () {
                                                                jQuery(this).toggleClass('active');
                                                                jQuery('.mega-navigation').toggleClass('active');
                                                        }
                                                );
                                        })

                                        window[tag].waitForElement('.accordionWrapper', function () {
                                                document.querySelectorAll('#mobile_menu2 > .accordionWrapper .accordionItemHeading').forEach(function (ele) {
                                                        ele.addEventListener('click', function (e) {
                                                                if (!e.target.closest('.accordionItem').classList.contains('open') && e.target.closest('.level-one')) {
                                                                        e.target.closest('.level-one').querySelector('.open') && e.target.closest('.level-one').querySelector('.open').classList.remove('open')
                                                                } else if (!e.target.closest('.accordionItem').classList.contains('open') && e.target.closest('.accordionWrapper')) {
                                                                        e.target.closest('.accordionWrapper').querySelector('.open') && e.target.closest('.accordionWrapper').querySelector('.open').classList.remove('open')
                                                                }
                                                                e.target.closest('.accordionItem').classList.toggle('open')
                                                        })

                                                })
                                        })

                                        window[tag].waitForElement('.et_pb_menu_inner_container .et_pb_menu__icon', function () {
                                                if (window.innerWidth <= 992) {
                                                        document.querySelector('.et_pb_column_3_tb_header.et_pb_css_mix_blend_mode_passthrough').insertAdjacentElement('afterbegin', document.querySelector('.et_pb_row.et_pb_row_0_tb_header.et_pb_row--with-menu'))
                                                        document.querySelector('.et_pb_menu_inner_container .et_pb_menu__icon').innerText = ''
                                                        document.addEventListener('click', function (e) {
                                                                if (e.target.closest('#page-container #et-boc')) {
                                                                        if (document.querySelector('.et_pb_menu__search-container--visible')) {
                                                                                document.querySelector('.et_pb_column_3_tb_header.et_pb_css_mix_blend_mode_passthrough .et_pb_column_0_tb_header .et_pb_image_0_tb_header').style.display = "none";
                                                                        }
                                                                        else {
                                                                                document.querySelector('.et_pb_column_3_tb_header.et_pb_css_mix_blend_mode_passthrough .et_pb_column_0_tb_header .et_pb_image_0_tb_header').style.display = "block";
                                                                        }
                                                                }
                                                        })
                                                }
                                        })


                                        window[tag].waitForElement('#mobile_menu2', function () {
                                                if (window.innerWidth <= 992) {
                                                        document.querySelector('.et_pb_column_4_tb_header .mobile_nav').insertAdjacentElement('afterend', document.querySelector('#mobile_menu2'))
                                                }
                                        })

                                },
                                50, 10000
                        );

                }

        })();


})();

